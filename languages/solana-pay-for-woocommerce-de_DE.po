msgstr "Transaktions-ID"

#: ../admin/partials/admin_payment_details.php:34,
#: ../public/partials/public_payment_details.php:34
msgid "Customer Wallet"
msgstr "Kundengeldbörse"

#: ../admin/partials/admin_payment_details.php:35,
#: ../public/partials/public_payment_details.php:35
msgid "Payment Amount"
msgstr "Zahlungsbetrag"

#: ../admin/partials/admin_payment_details.php:36,
#: ../public/partials/public_payment_details.php:36
msgid "Reference Account"
msgstr "Transaktionsadresse"

#: ../admin/partials/admin_tokens_table.php:63
msgid "Enable"
msgstr "Ermöglichen"

#: ../admin/partials/admin_tokens_table.php:64
msgid "Coin / Token"
msgstr "Coin / Token"

#: ../admin/partials/admin_tokens_table.php:65
msgid "Label"
